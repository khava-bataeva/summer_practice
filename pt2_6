def trip():
    a = float(input("Сколько километров хотите проехать на автомобиле? "))
    b = float(input("Сколько литров топлива расходует автомобиль на 100 километров? "))
    c = float(input("Сколько литров топлива в вашем баке? "))
    max_a = (c * 100) / b
    if max_a >= a:
        print("Вы сможете проехать желаемое расстояние")
    else:
        print("Вы не сможете проехать желаемое расстояние")
trip()